{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es6",
    "lib": [
      "es2019"
    ],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "outDir": "./dist",
    "rootDir": "./src",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
  }
}